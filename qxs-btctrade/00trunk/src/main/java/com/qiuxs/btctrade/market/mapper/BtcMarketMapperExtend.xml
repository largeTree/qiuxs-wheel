<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiuxs.btctrade.market.dao.BtcMarketDao">
		
		<!-- 获取平均卖价 -->
		<select id="calSellAvg" resultType="java.math.BigDecimal">
